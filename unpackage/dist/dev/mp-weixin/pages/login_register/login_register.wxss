.login_register .logo-area {
  padding: 0 40rpx;
  box-sizing: border-box;
}
.login_register .logo-area .help {
  width: 100%;
  height: 40rpx;
  text-align: right;
  overflow: hidden;
  padding-top: 24rpx;
}
.login_register .logo-area .help .image {
  width: 40rpx;
  height: 40rpx;
}
.login_register .logo-area .logo-img {
  text-align: center;
}
.login_register .logo-area .logo-img .image {
  width: 90rpx;
  height: 90rpx;
}
.login_register .logo-area .login-text {
  text-align: center;
}
.login_register .login-inp-area {
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-top: 60rpx;
}
.login_register .login-inp-area .login-inp {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #9b9b9b;
  border-bottom: 1px solid #ddd;
  padding: 20rpx 0;
  position: relative;
}
.login_register .login-inp-area .login-inp.err {
  border-bottom-color: #f66;
}
.login_register .login-inp-area .login-inp .label {
  height: 50rpx;
  line-height: 50rpx;
}
.login_register .login-inp-area .login-inp .input {
  border: none;
  outline: none;
  width: 70%;
  height: 50rpx;
  line-height: 50rpx;
  color: #000;
}
.login_register .login-inp-area .login-inp .login-inp-btn {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  right: 0;
}
.login_register .login-inp-area .error-des .icon-warn {
  vertical-align: middle;
  margin-right: 20rpx;
}
.login_register .login-inp-area .error-des .text {
  vertical-align: middle;
  color: #f66;
}
.login_register .login-inp-area .btns .btn {
  width: 100%;
  height: 94rpx;
  text-align: center;
  line-height: 94rpx;
  font-weight: bold;
}
.login_register .login-inp-area .btns .btn.register-btn {
  margin: 44rpx 0;
}
.login_register .login-inp-area .btns .btn .view {
  color: #7b7b7b;
  border-radius: 50rpx;
  background-color: #f0f0f0;
}
.login_register .login-inp-area .btns .btn .view.active {
  background-color: #ff6700;
  color: #fff;
}
.login_register .login-inp-area .yzm {
  text-align: center;
  margin-top: 30rpx;
  color: #9b9b9b;
}
.login_register .login-inp-area .yzm .gun {
  margin: 0 20rpx;
  color: #000;
}
.login_register .other {
  margin-top: 90rpx;
  text-align: center;
  font-size: 22rpx;
  color: #9b9b9b;
}
.login_register .other .other-text::before, .login_register .other .other-text::after {
  content: '';
  display: inline-block;
  width: 252rpx;
  height: 2rpx;
  background-color: #ccc;
  vertical-align: middle;
}
.login_register .other .other-text::before {
  margin-right: 5rpx;
}
.login_register .other .other-text::after {
  margin-left: 5rpx;
}
.login_register .other .other-items {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding-top: 20rpx;
}
.login_register .other .other-items .other-item {
  width: 55rpx;
  height: 55rpx;
  border-radius: 50%;
  margin: 20rpx;
  background-repeat: no-repeat;
  background-size: 70% 70%;
  background-position: center center;
}
.login_register .other .other-items .other-item.weibo {
  background-color: #ed9090;
  background-image: url(data:image/png;base64,);
}
.login_register .other .other-items .other-item.zhifubao {
  background-color: #6bb6ea;
  background-image: url(data:image/png;base64,);
  background-size: 80% 80%;
}
.login_register .other .other-items .other-item.weixin {
  background-color: #00be00;
  background-image: url(data:image/png;base64,);
}
.login_register .other .other-items .other-item.pingguo {
  background-color: #000;
  background-image: url(data:image/png;base64,);
}
.login_register .footer {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 100rpx;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  color: #9b9b9b;
}
.login_register .footer .nav-item:first-of-type {
  color: #000;
}
.login_register .footer .nav-item::after {
  content: '|';
  margin-left: 20rpx;
}
.login_register .footer .nav-item:last-of-type::after {
  content: '';
}

