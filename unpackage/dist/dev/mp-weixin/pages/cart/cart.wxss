.cart {
  background-color: #f2f2f2;
}
.cart .go-login {
  background-color: #fff;
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.cart .go-login .login-text {
  color: #ccc;
}
.cart .go-login .login-text .icon-xiangyou {
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  background-size: 100% 100%;
  vertical-align: middle;
  margin-left: 10rpx;
  background-image: url(data:image/png;base64,);
}
.cart .cart-area {
  background-color: #ebebeb;
  height: 122rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.cart .cart-area .icon-cart-space {
  width: 80rpx;
  height: 70rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.cart .cart-area .text {
  margin: 0 20rpx;
  color: #ccc;
}
.cart .cart-area .go-buy {
  width: 126rpx;
  height: 48rpx;
  border: 1rpx solid #ddd;
  box-sizing: border-box;
  text-align: center;
  -webkit-box-align: 48rpx;
  -webkit-align-items: 48rpx;
          align-items: 48rpx;
}
.cart .like .image {
  width: 100%;
  height: 120rpx;
}
.cart .like .list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background-color: #fff;
  padding-bottom: 100rpx;
  -webkit-animation: change 0.2s linear;
          animation: change 0.2s linear;
}
@-webkit-keyframes change {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes change {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
.cart .like .list .list-item {
  width: 49.5vw;
  margin-bottom: 20rpx;
}
.cart .like .list .list-item .image {
  height: 370rpx;
  width: 100%;
  /* margin-bottom: 10rpx; */
}
.cart .like .list .list-item .price .now-price {
  color: #ff6700;
}
.cart .like .list .list-item .price .old-price {
  color: #6f7575;
  margin-left: 10rpx;
  font-size: 20rpx;
  text-decoration: line-through;
}

