.search .search-header {
  height: 84rpx;
  background-color: #f2f2f2;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 30rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.search .search-header .icon {
  width: 40rpx;
  height: 40rpx;
  background-size: 100% 100%;
}
.search .search-header .icon.icon-xiangzuo {
  background-image: url(data:image/png;base64,);
}
.search .search-header .icon.icon-search {
  width: 50rpx;
  height: 50rpx;
  background-image: url(data:image/png;base64,);
}
.search .search-header .input {
  background-color: #fff;
  width: 548rpx;
  height: 46rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.search .title {
  margin-top: 50rpx;
  font-weight: bold;
  padding-left: 30rpx;
}
.search .search-bottom-img {
  width: 680rpx;
  height: 114rpx;
  border-radius: 20rpx;
  margin: 30rpx;
}
.search .rec-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 30rpx;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  font-size: 26rpx;
}
.search .rec-list .list-item {
  width: 260rpx;
  margin-bottom: 36rpx;
  margin-right: 44rpx;
  padding-left: 30rpx;
  color: #3c3d3e;
}
.search .rec-list .list-item .list-item-img {
  width: 56rpx;
  height: 26rpx;
  margin-left: 10rpx;
}

